<!--------------------------------------------------------------------	Base class that can be used to extend for other classes--------------------------------------------------------------------><!---   The Super keyword supports only one level of inheritance.  ---><cfcomponent name="GenericGetSet">	<cfset Variables.PropertyStruct = structNew()>		<cffunction name="set" output="false" access="public" returntype="any">		<cfargument	name="Property"	type="string" required="true"> 		<cfargument	name="Value" type="any"	required="true">				<cfif NOT IsSimpleValue(Arguments.Property)>			<cfthrow type="InvalidPropertyException" message="Property Name is not of type string">		</cfif>				<cfset StructInsert(Variables.PropertyStruct, Arguments.Property, Arguments.Value, 1)>		<cfreturn This>   </cffunction>      <cffunction name="get" output="false" access="public" returntype="any">		<cfargument	name="Property"	type="string" required="true"> 						<cfif NOT IsSimpleValue(Arguments.Property)>			<cfthrow type="InvalidPropertyException" message="Property Name is not of type string">		</cfif>				<cfif StructKeyExists(Variables.PropertyStruct,Arguments.Property)>							<cfreturn Variables.PropertyStruct[Arguments.Property]>		<cfelse>			<cfthrow type="PropertyNotDefinedException" message="Property Name has not been defined">		</cfif>		<cfreturn "">	   </cffunction>   </cfcomponent>